public class MethodEx {
	public void message()
	{
	    System.out.println("hello");
	    System.out.println("XYZ company");
	}

	public static void main(String[] args) {
		MethodEx obj= new MethodEx();
		obj.message();
		System.out.println("hello");
		System.out.println("world");
		System.out.println("Java");
		obj.message();
	}

}
